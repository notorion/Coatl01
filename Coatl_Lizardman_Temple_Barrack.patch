+	{
+		Npc boss1 = World.getInstance().getNpc(29413);
+		if (boss1 != null)
+		{
+			boss1.setInvul(true);
+		}
+	}
+	
+	public boolean isCoatlActive()
+	{
+		return spawnedMainBoss != null;
+	}
